pipeline {
    agent any
    options {
        skipDefaultCheckout true
    }

    stages {
        stage('Checkout') {
            steps {
                git url: "${params.GIT_REPOSITORY}", branch: "${params.GIT_BRANCH}", credentialsId: "${params.GIT_CREDENTIALS}"
            }
        }
        stage('List') {
            steps {
                sh "ls"
            }
        }
    }
}